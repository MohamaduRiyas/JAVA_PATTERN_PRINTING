import java.util.*;

public class Main{
    public static void main(String args[]){
        
        Scanner console = new Scanner(System.in);
        
        int num = console.nextInt();
        
        
        for(int i=1; i<=num; i++){
            for(int j=1; j<=i; j++){
                System.out.print(j);
            }
            System.out.println(" ");
        }
        
        console.close();
        
    }
}


//output

1 
12 
123 
1234 
12345 
