import java.util.*;

public class Main{
    public static void main(String args[]){
        
        Scanner console = new Scanner(System.in);
        
        int num = console.nextInt();
        
        
        for(int i=1; i<=num; i++){
            for(int j=1; j<=num; j++){
                System.out.print(i);
            }
            System.out.println(" ");
        }
        
        console.close();
        
    }
}

//output
11111 
22222 
33333 
44444 
55555 

